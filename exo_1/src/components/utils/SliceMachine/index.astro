---
import FullPicture from "../../molecules/FullPicture/index.astro"
import SmallPicture from "../../molecules/SmallPicture/index.astro"
import Description from "../../molecules/Description/index.astro"
import Pelicule from "../../molecules/Pelicule/index.astro"
import Video from "../../molecules/Video/index.astro"
const {
    component,
    options
} = Astro.props
if(component._type === "projectVideoDocumentType") console.log(component.video)
---
{
    component._type === "smallPictureDocumentType" && <SmallPicture url={component.picture.picture.asset.url} alterativeText={component.picture.alternativeText} />
}
{
    component._type === "fullPictureDocumentType" && <FullPicture url={component.picture.picture.asset.url} alterativeText={component.picture.alternativeText} />
}
{
     component._type === "descriptionDocumentType" && <Description textLeft={component.textLeft} textRight={component.textRight} pictureLeft={component.pictureLeft} pictureRight={component.pictureRight} />
}
{
     component._type === "projectVideoDocumentType" && <Video url={component.video.video.asset.url} alt="" />

}
{
     component._type === "sliderPictureDocumentType" &&<Pelicule images={component.images}/>
}
